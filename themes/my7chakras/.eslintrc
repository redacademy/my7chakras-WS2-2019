{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-alert": 0,
    "no-bitwise": 0,
    "camelcase": 1,
    "curly": 1,
    "eqeqeq": 1,
    "no-console": 1,
    "no-eq-null": 0,
    "guard-for-in": 1,
    "no-empty": 1,
    "quotes": [1, "single"]
  },
  "globals": {
    "jQuery": false,
    "$": false,
    "_": false
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  }
}
